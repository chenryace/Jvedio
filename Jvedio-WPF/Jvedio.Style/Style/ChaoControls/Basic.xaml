<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:chao="https://gitee.com/secret/chao-controls">





    <Style x:Key="NormalPathMsg" TargetType="{x:Type chao:PathMsg}">
        <Setter Property="Width" Value="30" />
        <Setter Property="Height" Value="30" />
        <Setter Property="Foreground" Value="{DynamicResource Global.Foreground}" />
        <Setter Property="HoverBackground" Value="{DynamicResource Window.Title.Hover.Background}" />
        <Setter Property="MsgHeight" Value="15" />
        <Setter Property="MsgMargin" Value="2" />
        <Setter Property="PathHeight" Value="16" />
    </Style>


</ResourceDictionary>